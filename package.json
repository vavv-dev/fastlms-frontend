{
  "name": "fastlms-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "build:prod": "tsc && vite build --mode prod",
    "lint": "tsc && eslint . --fix",
    "preview": "vite preview",
    "upgrade": "npx npm-check-updates -u",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,scss,md}'",
    "codemod:import": "npx @mui/codemod@latest v5.0.0/top-level-imports src/**/*.ts*",
    "openapi:generate": "openapi-ts",
    "i18n:extract": "i18next-scanner --config .i18next-scanner.config.cjs && rm -f src/locale/*/dummy.json",
    "prepare": "husky",
    "test": "echo \"No test yet. TODO\" && exit 1",
    "generate_source_file": "bash ./script/generate_source_file.sh"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.1.1",
    "@hookform/resolvers": "^3.9.1",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.3.1",
    "@mui/x-charts": "^7.23.2",
    "@tiptap/core": "^2.11.0",
    "@tiptap/extension-color": "^2.11.0",
    "@tiptap/extension-heading": "^2.11.0",
    "@tiptap/extension-highlight": "^2.11.0",
    "@tiptap/extension-image": "^2.11.0",
    "@tiptap/extension-link": "^2.11.0",
    "@tiptap/extension-placeholder": "^2.11.0",
    "@tiptap/extension-strike": "^2.11.0",
    "@tiptap/extension-subscript": "^2.11.0",
    "@tiptap/extension-superscript": "^2.11.0",
    "@tiptap/extension-table": "^2.11.0",
    "@tiptap/extension-table-cell": "^2.11.0",
    "@tiptap/extension-table-header": "^2.11.0",
    "@tiptap/extension-table-row": "^2.11.0",
    "@tiptap/extension-task-item": "^2.11.0",
    "@tiptap/extension-task-list": "^2.11.0",
    "@tiptap/extension-text-style": "^2.11.0",
    "@tiptap/extension-underline": "^2.11.0",
    "@tiptap/pm": "^2.11.0",
    "@tiptap/react": "^2.11.0",
    "@tiptap/starter-kit": "^2.11.0",
    "@uppy/aws-s3": "^4.1.3",
    "@uppy/dashboard": "^4.2.0",
    "@uppy/drag-drop": "^4.0.5",
    "@uppy/file-input": "^4.0.4",
    "@uppy/progress-bar": "^4.1.0",
    "@uppy/react": "^4.1.0",
    "emoji-picker-react": "^4.12.0",
    "i18next-browser-languagedetector": "^8.0.2",
    "jotai": "^2.11.0",
    "lodash": "^4.17.21",
    "mui-tiptap": "^1.17.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-i18next": "^15.4.0",
    "react-movable": "^3.4.0",
    "react-player": "^2.16.0",
    "react-reader": "^2.0.12",
    "react-rewards": "^2.0.4",
    "react-router-dom": "^7.1.1",
    "swr": "^2.3.0",
    "tiptap-extension-resize-image": "^1.2.1",
    "webvtt-parser": "^2.2.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@hey-api/openapi-ts": "^0.61.0",
    "@types/eslint": "^9.6.1",
    "@types/lodash": "^4.17.14",
    "@types/react": "^19.0.3",
    "@types/react-dom": "^19.0.2",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "babel-plugin-import": "^1.13.8",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "i18next-scanner": "^4.6.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "rollup": "^4.30.0",
    "rollup-plugin-visualizer": "^5.13.1",
    "terser": "^5.37.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.7"
  },
  "lint-staged": {
    "!(src/api/**/*).{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "!(src/api/**/*).{json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
